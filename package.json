{
	"name": "glipper",
	"version": "1.0.0",
	"description": "An easy utility for game clip cutting.",
	"main": "./out/main/index.js",
	"author": "Jakub Tabacek",
	"license": "GPL-3.0",
	"scripts": {
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
		"typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
		"typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
		"typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
		"start": "electron-vite preview",
		"dev": "electron-vite dev",
		"dev:watch": "electron-vite dev --watch",
		"build": "pnpm run typecheck && node copy-prisma-files.js && electron-vite build",
		"postinstall": "electron-builder install-app-deps",
		"build:unpack": "pnpm run build && electron-builder --dir",
		"build:win": "pnpm run build && electron-builder --win",
		"build:mac": "pnpm run build && electron-builder --mac",
		"build:linux": "pnpm run build && electron-builder --linux",
		"prisma:migrate:dev": "prisma migrate dev",
		"prisma:migrate:deploy": "prisma migrate deploy",
		"prisma:generate": "prisma generate",
		"prisma:studio": "prisma studio"
	},
	"dependencies": {
		"@electron-toolkit/preload": "^3.0.1",
		"@electron-toolkit/utils": "^3.0.0",
		"@ffmpeg-installer/ffmpeg": "^1.1.0",
		"@primevue/themes": "^4.0.7",
		"@prisma/client": "5.20.0",
		"@prisma/engines": "^5.20.0",
		"@vueuse/core": "^11.1.0",
		"date-fns": "^4.1.0",
		"electron-clipboard-ex": "^1.3.3",
		"electron-log": "^5.2.0",
		"electron-updater": "^6.3.4",
		"ffprobe": "^1.1.2",
		"ffprobe-static": "^3.1.0",
		"fluent-ffmpeg": "^2.1.3",
		"pinia": "^2.2.2",
		"primeicons": "^7.0.0",
		"primevue": "^4.0.7",
		"prisma": "^5.20.0",
		"tailwindcss-primeui": "^0.3.4"
	},
	"devDependencies": {
		"@electron-toolkit/eslint-config": "^1.0.2",
		"@electron-toolkit/eslint-config-ts": "^2.0.0",
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@rushstack/eslint-patch": "^1.10.4",
		"@types/ffprobe": "^1.1.8",
		"@types/ffprobe-static": "^2.0.3",
		"@types/fluent-ffmpeg": "^2.1.26",
		"@types/node": "^22.7.3",
		"@vitejs/plugin-vue": "^5.1.4",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"autoprefixer": "^10.4.20",
		"electron": "^32.1.2",
		"electron-builder": "^25.0.5",
		"electron-vite": "^2.3.0",
		"eslint": "^9.11.1",
		"eslint-plugin-vue": "^9.28.0",
		"fs-extra": "^11.2.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"replace-in-file": "^8.2.0",
		"tailwindcss": "^3.4.13",
		"typescript": "^5.6.2",
		"vite": "^5.4.8",
		"vue": "^3.5.9",
		"vue-tsc": "^2.1.6"
	},
	"packageManager": "pnpm@9.11.0+sha256.1c0e33f70e5df9eede84a357bdfa0b1f9dba6e58194628d48a1055756f553754"
}
